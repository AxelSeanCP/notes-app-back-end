{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "quotes": "off",
        "eslint-plugin-import/no-extraneous-dependencies": "off",
    }
}
